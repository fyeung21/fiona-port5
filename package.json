{
  "name": "fiona-port5",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "sass sass/styles.scss app/styles/styles.css",
    "build:remix": "remix vite:build",
    "dev": "run-p dev:*",
    "dev:css": "sass --watch sass/styles.scss app/styles/styles.css",
    "dev:remix": "remix vite:dev",
    "format": "prettier --write ./",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@remix-run/server-runtime": "^2.8.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/remix": "^2.8.0",
    "isbot": "^4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.0",
    "@remix-run/eslint-config": "^2.8.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
